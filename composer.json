{
  "name": "dpfaffenbauer/process-manager",
  "type": "pimcore-plugin",
  "license": "GPLv3",
  "description": "Process Manager helps you to see statuses for long running Processes",
  "keywords": [
    "pimcore",
    "pimcore-plugin"
  ],
  "homepage": "https://www.pfaffenbauer.at",
  "authors": [
    {
      "name": "Dominik Pfaffenbauer",
      "email": "dominik@pfaffenbauer.at",
      "homepage": "https://www.pfaffenbauer.at/",
      "role": "Developer"
    }
  ],
  "require": {
    "pimcore/installer-plugin": ">=1"
  },
  "autoload": {
    "psr-4": {
      "ProcessManager\\": "lib"
    }
  },
  "requires": {
    "mtdowling/cron-expression": "^1.1"
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "ProcessManagerBundle\\ProcessManagerBundle"
      ]
    },
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}